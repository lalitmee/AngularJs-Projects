    "karma-coverage": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "semver": "~6.3.0",
    "should": "~13.2.3",
    "supertest": "~4.0.2"
