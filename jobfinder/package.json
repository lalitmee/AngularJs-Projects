    "express-session": "^1.16.2",
    "generate-password": "~1.4.1",
    "glob": "~7.1.4",
    "gulp": "~4.0.2",
    "gulp-angular-templatecache": "~3.0.0",
    "gulp-autoprefixer": "~7.0.1",
    "gulp-concat": "~2.6.1",
    "gulp-csslint": "~1.0.1",
    "gulp-csso": "~3.0.1",
    "gulp-eslint": "^6.0.0",
